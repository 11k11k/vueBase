{"ast":null,"code":"import \"core-js/modules/es.array.unshift.js\";\nimport TodoHeader from './components/TodoHeader.vue';\nimport TodoMain from './components/TodoMain.vue';\nimport TodoFooter from './components/TodoFooter.vue';\nexport default {\n  data() {\n    return {\n      listTable: [{\n        id: 1,\n        name: \"打篮球\"\n      }, {\n        id: 2,\n        name: '羽毛球'\n      }, {\n        id: 3,\n        name: '排球'\n      }]\n    };\n  },\n  components: {\n    TodoHeader: TodoHeader,\n    TodoMain: TodoMain,\n    TodoFooter: TodoFooter\n  },\n  methods: {\n    ListAdd(value) {\n      this.listTable.unshift({\n        id: +new Date(),\n        name: value\n      });\n    },\n    dec(value) {\n      // console.log(value);\n      this.listTable = this.listTable.filter(item => item.id !== value);\n    },\n    clear() {\n      this.listTable = [];\n    }\n  },\n  watch: {\n    listTable: {\n      deep: true,\n      handler(newValue) {\n        localStorage.setItem(\"listkey\");\n      }\n    }\n  }\n};","map":{"version":3,"names":["TodoHeader","TodoMain","TodoFooter","data","listTable","id","name","components","methods","ListAdd","value","unshift","Date","dec","filter","item","clear","watch","deep","handler","newValue","localStorage","setItem"],"sources":["src/App.vue"],"sourcesContent":["<template>\r\n  <!-- 主体区域 -->\r\n  <section id=\"app\">\r\n    <!-- 输入框 -->\r\n    <!-- <header class=\"header\">\r\n      <h1>小黑记事本</h1>\r\n      <input placeholder=\"请输入任务\" class=\"new-todo\" />\r\n      <button class=\"add\">添加任务</button>\r\n    </header> -->\r\n    <TodoHeader @add=\"ListAdd\"></TodoHeader>\r\n    <!-- 列表区域 -->\r\n    <!-- <section class=\"main\">\r\n      <ul class=\"todo-list\">\r\n        <li class=\"todo\">\r\n          <div class=\"view\">\r\n            <span class=\"index\">1.</span> <label>吃饭饭</label>\r\n            <button class=\"destroy\"></button>\r\n          </div>\r\n        </li>\r\n      </ul>\r\n    </section> -->\r\n    <TodoMain :list